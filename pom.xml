        <duapp-api.version>0.5.1-SNAPSHOT</duapp-api.version>
        <duapp-user-api.version>0.0.14</duapp-user-api.version>
        <ofc-admin-api.version>0.0.4-SNAPSHOT</ofc-admin-api.version>
        <carrier-integrator-api.version>1.0.7-RELEASE</carrier-integrator-api.version>
